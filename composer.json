{
    "name": "graham-campbell/cloudflare-api",
    "description": "CloudFlare API Is A CloudFlare API Client For Laravel 4.1",
    "keywords": ["laravel", "framework", "cloudflare api", "cloudflare", "api", "CloudFlare API", "CloudFlare-API", "Laravel CloudFlare API", "Laravel-CloudFlare-API", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/event-dispatcher": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "graham-campbell/core-api": "0.5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\CloudFlareAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\CloudFlareAPI\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
