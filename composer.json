{
    "name": "graham-campbell/cloudflare-api",
    "description": "CloudFlare API Is A CloudFlare API Client For Laravel 4.1/4.2",
    "keywords": ["laravel", "framework", "cloudflare api", "cloudflare", "api", "CloudFlare API", "CloudFlare-API", "Laravel CloudFlare API", "Laravel-CloudFlare-API", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/support": "~4.1",
        "graham-campbell/manager": "~1.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/guzzle-services": "0.4.*",
        "guzzlehttp/retry-subscriber": "~2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\CloudFlareAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\CloudFlareAPI\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-experimental": "0.6-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
